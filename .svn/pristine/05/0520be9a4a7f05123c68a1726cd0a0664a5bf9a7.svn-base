package com.jcl.pojo;

import java.math.BigDecimal;
import java.util.List;

import com.sun.istack.internal.NotNull;

public class SubzhObject extends Subzh{
	
	private List<SubzhContract> subzhContractList;
	
	private String flag;
	
	
	private BigDecimal cost;
	
	private String contractCode;
	
	private String contractName;
	
	private String schemes;//保存页面传过来的方案字符串信息
	
	public List<SubzhContract> getSubzhContractList() {
		return subzhContractList;
	}

	public void setSubzhContractList(List<SubzhContract> subzhContractList) {
		this.subzhContractList = subzhContractList;
	}

	public String getFlag() {
		return flag;
	}

	public void setFlag(String flag) {
		this.flag = flag;
	}

	public BigDecimal getCost() {
		return cost;
	}

	public void setCost(BigDecimal cost) {
		this.cost = cost;
	}

	public String getContractCode() {
		return contractCode;
	}

	public void setContractCode(String contractCode) {
		this.contractCode = contractCode;
	}

	public String getContractName() {
		return contractName;
	}

	public void setContractName(String contractName) {
		this.contractName = contractName;
	}

	public String getSchemes() {
		return schemes;
	}

	public void setSchemes(String schemes) {
		this.schemes = schemes;
	}
	
	
}
