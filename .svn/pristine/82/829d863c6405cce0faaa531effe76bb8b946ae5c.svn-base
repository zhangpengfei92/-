@charset "utf-8";
/* CSS Document */
html {overflow-y:scroll;background:#fff;-webkit-text-size-adjust:none}
html, body {overflow-x: hidden;width: 100%; height:100%; padding: 0; margin: 0;}
body,h1,h2,h3,h4,ul,ol,dl,dd,p,input,form,textarea,pre,fieldset,section{ margin:0;font-size:100%;line-height:1;}
ul,ol,form,textarea,fieldset,legend { padding:0}
ul,ol { list-style:none}
img { -ms-interpolation-mode:bicubic;height:auto}
textarea { overflow-y:auto;resize:none}
:focus { outline:0 none}
fieldset,select,input,textarea,img { border:0}
a{color:#333}
a,a:hover { text-decoration:none;-webkit-appearance:none;appearance:none}
.fix:after { content:".";height:0;visibility:hidden;display:block;clear:both}
.fix { display:block}
article,aside,details,figcaption,figure,footer,header,hgroup,nav,section,menu,div,main{ display:block}
em,i { font-style:normal}
input[type="search"],input[type="submit" ],input[type="reset"],input[type="button"],button { -webkit-appearance:none}
body {font-size:100%;color:#333;}
body,input,select{font-family:"helvetica neue","微软雅黑";}
/*body { -moz-user-select:none; -webkit-user-select:none; -ms-user-select:none; -khtml-user-select:none; user-select:none}*/
a,div,p,dl,h1,h2,h3,dt,dd,i,em,b,strong,section,header,article,footer,nav,ul,li,span,menu { -webkit-tap-highlight-color:transparent;-webkit-touch-callout:none; -webkit-user-select:none;}
table{border-collapse: collapse;}
tr td{ padding:0; margin:0;}
.grey{ background:#f5f5f5;}
/* Public */
.mobile_wrap * { -moz-box-sizing:border-box; -webkit-box-sizing:border-box; box-sizing:border-box;}
.y_array,.y_c,.x_c,.xy_cent { width:100%; display:-webkit-box; display:-moz-box; display:box;}
.td { -webkit-box-flex:1; -moz-box-flex:1; box-flex:1}/* 定义两个可伸缩的元素 */
.y_array { -webkit-box-orient:vertical;-moz-box-orient:vertical;box-orient:vertical}/* 从上向下垂直排列子元素 */
.y_c { -webkit-box-align:center;-moz-box-align:center;box-align:center}/* 子元素垂直居中 */
.x_c { -webkit-box-pack:center;-moz-box-pack:center;box-pack:center}/* 子元素水平居中 */
.xy_cent{ -webkit-box-align:center;-moz-box-align:center;box-align:center;-webkit-box-pack:center;-moz-box-pack:center;box-pack:center}
/*start*/
/*头部-top*/
.category{padding-bottom:6em;}
.ls-header {background: #fff;color: #000;}
.ls-header {height: 3.7em;line-height: 3.7em;position: relative;text-align: center;z-index: 2;}
.ls-header .ls-header-left {float: left;}
.ls-header .ls-header-left a, .ls-header .ls-header-right a {display: block;width: 4em;height: 3.5em;line-height: 3em;}
.ls-header .ls-header-left a i.icon-back{background:url(../images/Backt.png) no-repeat center center;display: block;width: 80%;height: 100%;margin: 0;background-size: 1.0em auto;}
.ls-header .ls-header-title {position: absolute;top: 0;left: 4em;right: 4.0em;bottom: 0;font-size: 1.4em;}
.paylist{ margin:0.5rem;}
.paylist li{ overflow:hidden; padding:0.8rem 0.2rem;border-bottom:1px solid #e9e9e9; cursor:pointer;}
.payicon{ float:left; width:80%; overflow:hidden;}
.payicon p{ float:left; padding-right:0.5rem;font-size:1.1rem; }
.payicon p.icon02{padding-top:0.3rem;}
.payicon p i{ display:block; font-size:0.9rem; color:#999;padding-top:0.3rem;}
.payicon p.icon01{ width:15%; display:inline-block; }
.payicon p.icon01 img{ width:100%;}
.payicon p.icontit{padding-top:0.8rem;}
#container{ float:right; width:10%; line-height:3rem;}
.rebut {background: #3a74c5;color: #fff;text-align: center;border-radius: 4px;padding: 2.5% 0; border:none;
        width: 100%;font-size: 1.3rem;margin-top:2rem;font-family: "Microsoft YaHei";display: block; cursor:pointer}

/*支付页面*/
.Monbox{ background:#fff;}
.paymon .mtit{ float:left; font-size:1.3rem; padding-top:0.2rem;}
.paymon .maccount{ float:right; font-size:1.5rem;}
.paymon .maccount span{ color:#C00; font-size:1.8rem;}
.paymon{ overflow:hidden; padding:5%; border-bottom:1px solid #e9e9e9; text-align:center;}
.paymon p{ line-height:240%; font-size:1.2rem;}
.paymon p a.consu{ padding:0.3rem 0.8rem;background:#f2f2f2;border:1px solid #e9e9e9; font-size:1.1rem; p}
.paybox2 h3{ font-size:1.2rem; padding:3% 5%; color:#333; font-weight:normal;}
.paybox2 h3 em{ text-decoration:underline; color:#fc4847; padding-left:0.5rem;}
.paymethod2 {background: #fff;border: #e9e9e9 1px solid;}
.paylist2{ margin:0.5rem 5%;}
.rebut2{ margin:5%; background:#fe504f; width:90%;}

/*购买*/
.paymon i{ float:left; display:inline-block; padding-top:0.5rem;font-size: 1.2rem;}
.paymon i.icon{padding-left:0.5rem;width:10%; padding-top:0.3rem;}
.paymon i.icon img{ width:80%;}
.icon02{ width:10%; display:inline-block;}
.icon02 img{ display:block; width:80%;}
.icontit2{ padding-top:0.5rem;}
.wpayicon{width:100%;}
.raccount{ float:right; border-radius:30px; display:inline-block; width:20%; border:0.1rem solid #fc4847;color:#fc4847; text-align:center; padding:1% 0; margin-top:0.4rem;}
.paction{ margin:5%; color:1.4rem;color:#333;}
.num{color:#C00;}
.Gaction{float:right; display:inline-block; padding-top:0.5rem;font-size: 1.1rem;color:#999;}
.selected{background:#fd4543; color:#fff;}
.raccount:hover{background:#fd4543; color:#fff;}

/*我要购买*/
.mybuy{order-top:1px solid #e9e9e9;}
.mybuy li{overflow:hidden; padding:0.8rem 0.2rem;border-bottom:1px solid #e9e9e9; overflow:hidden;}
.mybuy li .buynum{ float:left; width:35%; padding-left:5%; }
.mybuy li .buynum p{ font-size:1.3rem; color:#000;}
.mybuy li .buynum p.mouth{color:#999; font-size:1.1rem; padding:5% 0;}
.mybuy li .buynum p.red{color:#C00;font-size: 1.0rem;}
.mybuy li .buybut{ float:right; width:20%}
.mybuy li .buybut a{ background:#ffa119; display:block; width:5rem; height:2.8rem; line-height:2.8rem;color:#fff; text-align:center; font-size:1.3rem; margin-top:1.5rem;}
.mybuy li.tit{ background:#f5f5f5;}
.mybuy li span{ display:inline-block; width:30%; font-size:1.2rem;color:#666; padding:0.2rem 0;}
.mybuy li span.name{ text-align:left; padding-left:0.5rem;}
.mybuy li span.buy{text-align:center;}
.mybuy li span.mon{ text-align:right;}
.mybuy li span.namec{ color:#111;}
.mybuy li span.buyc{color:#999;}
.mybuy li span.monc{color:#ffa119;}