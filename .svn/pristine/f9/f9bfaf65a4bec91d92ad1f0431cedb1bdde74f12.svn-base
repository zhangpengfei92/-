.Tratable table{border-bottom:1px solid #e9e9e9;}
.Tratable table tr td{ padding:13px 10px; font-size:12px; border-top:1px solid #e9e9e9;color:#333;}
.Tratable table tr td span{ display:inline-block; border:1px solid #ccc;color:#333; padding:4px 15px; margin-right:10px; cursor:pointer;}
.Tratable table tr td.green{color:#0C6;}
.Tratable table tr td.blue{color:#0CC;}
.Tratable table tr.tit{background:#e9e9e9; font-size:14px; color:#111;}
.Tratable table tr.Tinput{background:#F2F2F2;}
.Tinput{ overflow:hidden;}
.Tinput .tabline{ padding-right:10px; float:left;}
.Tinput label{float:left; width:90px; text-align:right;line-height:24px; height:24px;font-size:14px; padding-right:5px;}
.Tinput input{float:left; border:1px solid #ddd; line-height:24px; height:24px; padding:0 5px; width:120px; }
.cxbutton{ background:#920916; color:#fff; height:26px; line-height:26px; width:70px; text-align:center;}
.Tinput i.line{ display:inline-block; float:left; padding:0 5px;color:#ddd;line-height:24px; height:24px;}
#parentab{ float:left; background: url('http://ourjs.github.io/static/2015/arrow.png') right center no-repeat #fff; 
   /* the width and the height of your image */ width:120px; line-height:24px; height:24px;overflow: hidden;border: solid 1px #ccc;}
#parentab select{-webkit-appearance:none;-moz-appearance:none;appearance:none;background:transparent;border:none;padding-left:5px;width:140px;height:100%; padding-bottom:6px;}
.Tinput button{background:#920916; color:#fff; width:70px; height:26px; line-height:20px; text-align:center;}
.Tinput button.mrbut{ background:#fff;color:#333; border:#999 1px solid;}
.Tratable a{ color:#39F;}
.Tratable em{ color:#999; padding:0 10px;}
.Tratable i{ padding-right:5px;}

