/*后台管理*/
/*会员列表*/
.Matopbox{border-bottom:2px solid #920916;/*width:expression(document.body.clientWidth <=1204? "1204px": "auto");*/min-width:1200px;z-index: 66;width: 100%;height: 50px;background-color:#fff;line-height: 50px;}
