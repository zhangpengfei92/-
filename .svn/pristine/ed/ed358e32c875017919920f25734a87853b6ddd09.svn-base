.Wtup p.Clear{ float:right; width:5rem;}
.Wtdown dl dd.Gold{width:50%; text-align:center; font-size:1.5rem;}
.Wtdown dl dd.Gold span{ padding-right:0;}
.Rgrey{ line-height:220%; font-size:1.5rem;}
.Qcbut{ background:#3492dc; width:96%;}
.Zsum{ background:#1f2025;border-top:0.4rem solid #000000; text-align:center;color:#b5b4b9; padding:2rem 0; font-size:1.6rem;}
