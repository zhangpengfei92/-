@charset "utf-8";
/* CSS Document */
html {overflow-y:scroll;background:#fff;-webkit-text-size-adjust:none}
html, body {overflow-x: hidden;width: 100%; height:100%; padding: 0; margin: 0;}
body,h1,h2,h3,h4,ul,ol,dl,dd,p,input,form,textarea,pre,fieldset,section{ margin:0;font-size:100%;line-height:1;}
ul,ol,form,textarea,fieldset,legend { padding:0}
ul,ol { list-style:none}
img { -ms-interpolation-mode:bicubic;height:auto}
textarea { overflow-y:auto;resize:none}
:focus { outline:0 none}
fieldset,select,input,textarea,img { border:0}
a{color:#333}
a,a:hover { text-decoration:none;-webkit-appearance:none;appearance:none}
.fix:after { content:".";height:0;visibility:hidden;display:block;clear:both}
.fix { display:block}
article,aside,details,figcaption,figure,footer,header,hgroup,nav,section,menu,div,main{ display:block}
em,i { font-style:normal}
input[type="search"],input[type="submit" ],input[type="reset"],input[type="button"],button { -webkit-appearance:none}
body {font-size:100%;color:#333;}
body,input,select{font-family:"helvetica neue","微软雅黑";}
/*body { -moz-user-select:none; -webkit-user-select:none; -ms-user-select:none; -khtml-user-select:none; user-select:none}*/
a,div,p,dl,h1,h2,h3,dt,dd,i,em,b,strong,section,header,article,footer,nav,ul,li,span,menu { -webkit-tap-highlight-color:transparent;-webkit-touch-callout:none; -webkit-user-select:none;}
table{border-collapse: collapse;}
tr td{ padding:0; margin:0;}
/* Public */
.mobile_wrap * { -moz-box-sizing:border-box; -webkit-box-sizing:border-box; box-sizing:border-box;}
.y_array,.y_c,.x_c,.xy_cent { width:100%; display:-webkit-box; display:-moz-box; display:box;}
.td { -webkit-box-flex:1; -moz-box-flex:1; box-flex:1}/* 定义两个可伸缩的元素 */
.y_array { -webkit-box-orient:vertical;-moz-box-orient:vertical;box-orient:vertical}/* 从上向下垂直排列子元素 */
.y_c { -webkit-box-align:center;-moz-box-align:center;box-align:center}/* 子元素垂直居中 */
.x_c { -webkit-box-pack:center;-moz-box-pack:center;box-pack:center}/* 子元素水平居中 */
.xy_cent{ -webkit-box-align:center;-moz-box-align:center;box-align:center;-webkit-box-pack:center;-moz-box-pack:center;box-pack:center}
/*start*/
/*头部-top*/
.category{padding-bottom:6rem;}
.category2{padding-bottom:0;}
.ls-header {background: #201f24;color: #fff;}
.ls-header {height:4rem;line-height:4rem;position: relative;text-align: center;z-index: 2;}
.ls-header .ls-header-left {float: left;}
.ls-header .ls-header-left a, .ls-header .ls-header-right a {display: block;width: 4rem;height:4rem;line-height: 3rem;}
.ls-header .ls-header-left a i.icon-back{background:url(../images/back.png) no-repeat center center;display: block;
width: 80%;height: 100%;margin: 0;background-size: 1.0rem auto;}
.ls-header .ls-header-title {position: absolute;top: 0;left: 4rem;right: 4.5rem;bottom: 0;font-size: 1.6rem;}
.ls-header .ls-header-right{ float:right;}
.ls-header .ls-header-right a i.icon-flash{ background:url(../images/flash.png) no-repeat center center;display: block;
width:100%;height: 100%;margin: 0;background-size: 2.0rem auto;}
.bggray{ background:#000000;}
.gray{ background:#eef0ef;}
.bgray{background:#201f24;}

/*撤单*/
.Tradebox ul.Ttab{ overflow:hidden; background:#201f24;border-top:1px solid #302f34;border-bottom: 1px solid #302f34;}
.Tradebox ul.Ttab li{ float:left; width:20%; line-height:300%; text-align:center;}
.Tradebox ul.Ttab li a{ display:inline-block;border-bottom:4px solid #201f24;font-size:1.4rem; color:#fff;}
.Tradebox ul.Ttab li a:hover{border-bottom:4px solid #3492dc;color:#3492dc;}
.Tradebox ul.Ttab li a.on{border-bottom:4px solid #3492dc;color:#3492dc; }
.jtab tr td{ border-bottom:1px solid #2f2f2f; text-align:center;color:#fff; padding:1.2rem 0;}
.jtab tr{font-size:1.2rem; padding:0 15px;}
.jtab tr.tit{ font-size:1.2rem;}
.jtab tr td.line{ border-right:1px solid #323232;padding:1rem 0;border-top: 1px solid #323232;}
.jtab tr td.line2{padding:2% 0;}
.jtab tr td span{ display:block; line-height:120%;}
.red{color:#de3f2c;}
.green{color:#21831e;}
.grey{color:#b9b9b9;}
.cbut{border-radius:5px; display:block; width:90%; text-align:center; background:#e23c30; color:#fff; line-height:220%; font-size:1.2rem;vertical-align:middle;-webkit-border-radius:5px;
-moz-border-radius:5px;border-radius:5px;behavior: url(js/PIE.htc);position:relative;overflow:hidden; cursor:pointer;}
.Gcbut{background:#21831e;}
.Sbut{background:#d6d6d6;color:#000;}
.wbut{ background:#424450;color:#fff;}

/*查询*/
.search{background:#fff;margin-top:0.6rem;}
.search ul{ overflow:hidden;}
.search ul li.ctitm{line-height: 2rem;height: 2rem;font-size:1.4rem; padding:1.0rem; font-weight:normal;border-bottom:0.4rem solid #000000; overflow:hidden; background:#201f24;}
.search ul li.ctitm a{ display:block; overflow:hidden;color:#fff;}
.search ul li.ctitm span.rarrow{ float:right;display: block;width:1.8rem;height:1.8rem;line-height:1.8rem;}
.search ul li.ctitm span.rarrow i.rimg{background:url(../images/next.png) no-repeat center center;display: block;width: 80%;height: 100%; margin: 0;background-size:0.6em auto;}

/*当日成交*/
.Cjbox ul{border-top:2px solid #000;}
.Cjbox ul li{border-bottom:5px solid #000; padding:5% 3%; background:#201f24;color:#fff; overflow:hidden;}
.Cjbox ul li .Gname{ width:30%; float:left;}
.Cjbox ul li .Gname p{ padding:0.3rem 0; text-align:center; font-size:1.3rem;}
.Cjbox ul li .Gname p.Buton{ margin:0 20%;}
.Cjbox ul li .Line{ float:left;background:#323136; height:7rem; width:1px;}
.Cjbox ul li .Gnum{ float:right; width:65%;}
.Cjbox ul li .Gnum p{padding:0.3rem 0; font-size:1.3rem; overflow:hidden;}
.Cjbox ul li .Gnum p span{ float:left; display:inline-block;color:#b9b9b9; padding-left:8%;}
.Cjbox ul li .Gnum p em{ float:right;}

/*当日委托*/
.Wtup{ overflow:hidden; font-size:1.2rem; }
.Wtup p.Wleft{ float:left; overflow:hidden;line-height:220%; width:60%;}
.Wtup p.Wleft a{ width:4rem; float:left;}
.Wtup p.Wleft span{ padding-left:3%;}
.Wtup p.time{float:right;line-height:220%; }
.Wtdown{ padding-top:5%; font-size:1.2rem;}
.Wtdown dl{ overflow:hidden;}
.Wtdown dl dd{ float:left; width:30%;}
.Wtdown dl dd.One{ width:40%;}
.Wtdown dl dd.Two{ width:18%; padding-top:2%; float:right;}
.Wtdown dl dd p{ line-height:160%;}
.Wtdown dl dd span{ padding-right:5%;}

/*历史委托*/
.rl input{ height:2.4rem; line-height:2.4rem; background:#1a191e;border:1px solid #313035; width:38%;}
.rl span{ width:6%; text-align:center;color:#313035;}
.rl i{ display:inline-block; float:right; width:15%;border-radius:0;}

/*策略账户*/
.Caccount{ background:#fff; margin:5px 0; font-size:1.4rem;}
.Caccount ul li{border-bottom:1px solid #ddd; padding:4% 3%; overflow:hidden;}
.Caccount ul li span{ float:left;height:2.4rem; line-height:2.4rem;}
.Caccount ul li em{ float:left; font-size:1.2rem;height:2.4rem; line-height:2.4rem;}
.Caccount ul li p{ display:inline-block; float:right;}
.Caccount ul li p input{ float:left;height:2.4rem; line-height:2.4rem; padding-right:3%;width:100%; text-align:right;}
.Caccount ul li p.Bnum{ position:relative;}
.Caccount ul li p.Bnum span{ position:absolute; right:0; top:0; cursor:pointer;}
.Caccount ul li p.Bnum span img{ width:100%;}

/*设置新密码*/
.Caccount ul li span.Swidth{ display:inline-block; width:7rem;}
.Suc{ margin:3%; background:#2f93df; color:#fff; height:3.6rem; line-height:3.6rem; border:none; width:94%; font-size:1.5rem;vertical-align:middle;-webkit-border-radius:5px;
-moz-border-radius:5px;border-radius:5px;behavior: url(js/PIE.htc); cursor:pointer;}

/*忘记密码*/
.Qus{ float:right;padding:0.3rem 0.7rem; font-size:1.4rem;}
.Qus a{color:#3492dc; }
.Caccount ul li p span.Get{color:#3492dc; cursor:pointer; float:right;}

/*注册*/
.ls-header .ls-header-left a i.icon-close{background:url(../images/guanbi.png) no-repeat center center;display: block;
width: 80%;height: 100%;margin: 0;background-size: 3.0rem auto;}
.regbox{color:#fff;}
.regbox h3{ font-size:1.8rem; padding:8% 3%; font-weight:normal;}
.regbox ul{ margin-bottom:2rem;}
