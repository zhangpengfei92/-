.regbox ul li span{ display:inline-block; width:7rem; height:3.0rem; line-height:3.0rem; float:left; width:22%;}
.regbox ul li input{ background:#201f24;color:#b2b1b6;height:3.0rem; line-height:3.0rem; float:left; width:35%;}
.regbox ul li em{ display:inline-block; width:3.0rem;height:3.0rem;float:right; cursor:pointer; padding-top:0.2rem;}
.regbox ul li em img{ width:100%; vertical-align:middle;}
.regbox ul li em.Get{color:#3492dc; width:7rem; line-height:3.0rem; float:right;}
.selectone{ float:left; width:25%; height:3rem; line-height:3rem; margin-right:1.5rem; background:url(../images/wdown.png) no-repeat right center; overflow:hidden;}
.selectone select{background:none;color:#fff; width:122%;height:3rem; line-height:3rem; font-size:1.5rem;}
.selectone select option{ background:#201f24;}
.Tk{ text-align:center; padding:5% 0; font-size:1.3rem; color:#b2b1b6;}
.Tk a{color:#3492dc;}

/*资金*/
.Wtup p.Wleft a.Cgq{ display:inline-block; width:3rem;}
