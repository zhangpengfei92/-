
.searchMeme-container{width:260px;}
.searchMeme-container div
{ margin: 0; padding: 0;}
.searchMeme-button-icon{background:url(../image/sear.png) 10px center;width:47px;height:45px;background-repeat: no-repeat;}
.searchMeme-button-searching{background-image:url(../image/loading.gif); position: relative;top: 33%;left: 36%;width: 60px;height: 50px;background-repeat: no-repeat;}
.orange-normal{border:1px solid #dbd0e0;}
.orange-normal:hover{border:1px solid #dbd0e0;}
.searchMeme-input-left input{width:240px;background-color: #65337b;border: none;color: #fff; z-index: 1;font-size:14px; height:47px;padding:0 5px; line-height:47px;}
.searchMeme-input-left{float:left;width:240px;overflow:hidden;height:47px;}
.searchMeme-button-left{width:47px;height:45px;cursor: pointer;float: left; z-index: 100;border:1px solid #dbd0e0;}
.searchMeme-input-right input{width:240px;background-color: #2D2822;border: none;color: #fff;z-index: 1; height:47px; line-height:47px; padding:0 5px;font-size: 14px;}
.searchMeme-input-right{float:right;height:45px;width: 240px;overflow:hidden;}
.searchMeme-button-right{width:47px;height:45px;cursor: pointer;float: right;z-index: 100;}





