@charset "utf-8";
/* CSS Document */
html {overflow-y:scroll;background:#fff;-webkit-text-size-adjust:none}
html, body {overflow-x: hidden;width: 100%; height:100%; padding: 0; margin: 0;}
body,h1,h2,h3,h4,ul,ol,dl,dd,p,input,form,textarea,pre,fieldset,section{ margin:0;font-size:100%;line-height:1;}
ul,ol,form,textarea,fieldset,legend { padding:0}
ul,ol { list-style:none}
img { -ms-interpolation-mode:bicubic;height:auto}
textarea { overflow-y:auto;resize:none}
:focus { outline:0 none}
fieldset,select,input,textarea,img { border:0}
a{color:#333}
a,a:hover { text-decoration:none;-webkit-appearance:none;appearance:none}
.fix:after { content:".";height:0;visibility:hidden;display:block;clear:both}
.fix { display:block}
article,aside,details,figcaption,figure,footer,header,hgroup,nav,section,menu,div,main{ display:block}
em,i { font-style:normal}
input[type="search"],input[type="submit" ],input[type="reset"],input[type="button"],button { -webkit-appearance:none}
body {font-size:100%;color:#333;}
body,input,select{font-family:"helvetica neue","微软雅黑";}
/*body { -moz-user-select:none; -webkit-user-select:none; -ms-user-select:none; -khtml-user-select:none; user-select:none}*/
a,div,p,dl,h1,h2,h3,dt,dd,i,em,b,strong,section,header,article,footer,nav,ul,li,span,menu { -webkit-tap-highlight-color:transparent;-webkit-touch-callout:none; -webkit-user-select:none;}
table{border-collapse: collapse;}
tr td{ padding:0; margin:0;}
/* Public */
.mobile_wrap * { -moz-box-sizing:border-box; -webkit-box-sizing:border-box; box-sizing:border-box;}
.y_array,.y_c,.x_c,.xy_cent { width:100%; display:-webkit-box; display:-moz-box; display:box;}
.td { -webkit-box-flex:1; -moz-box-flex:1; box-flex:1}/* 定义两个可伸缩的元素 */
.y_array { -webkit-box-orient:vertical;-moz-box-orient:vertical;box-orient:vertical}/* 从上向下垂直排列子元素 */
.y_c { -webkit-box-align:center;-moz-box-align:center;box-align:center}/* 子元素垂直居中 */
.x_c { -webkit-box-pack:center;-moz-box-pack:center;box-pack:center}/* 子元素水平居中 */
.xy_cent{ -webkit-box-align:center;-moz-box-align:center;box-align:center;-webkit-box-pack:center;-moz-box-pack:center;box-pack:center}
/*start*/
.tk {width: 100%;height: 100%;}
.swiper-container {width: 100%;height: 100%;}
.swiper-slide {height: auto;}
.bgray{background:#1a1a22;}
.cgrey {background: #1a1a22;}
.category{padding-bottom:6rem;}
/*header*/
.ls-header {background: #1a1a22;color: #fff;}
.ls-header {height:4rem;line-height:4rem;position: relative;text-align: center;z-index: 2;}
.ls-header .ls-header-left {float: left;}
.ls-header .ls-header-left a, .ls-header .ls-header-right a {display: block;width: 4rem;height:4rem;line-height: 4rem;}
.ls-header .ls-header-left a i.icon-back{background:url(../images/back.png) no-repeat center center;display: block;
width: 80%;height: 100%;margin: 0;background-size: 1.2rem auto;}
.ls-header .ls-header-title {position: absolute;top: 0;left: 4rem;right: 4.5rem;bottom: 0;font-size: 1.8rem;}
.ls-header .ltit{ float:left; width:6rem; height:3.2rem; padding-top:0.4rem;}
.ls-header .ltit span{ display:block; text-align:left; height:1.6rem; line-height:1.6rem; font-size:1.2rem;}
.ls-header .ltit span.yellow{color:#ffd010;}
.ls-header .ls-header-right{ float:right;}
.ls-header .ls-header-right a i.icon-flash{background:url(../images/jy/shuaxin.png) no-repeat center center;display: block;
width:100%;height: 100%;margin: 0;background-size: 2.0rem auto;}
.ls-header .ls-header-right a i.icon-dott{ background:url(../images/jy/gengduo.png) no-repeat center center;display: block;
width:4rem;height:4rem;margin: 0;background-size: 2.6rem auto;}
.ls-header .ls-header-right a.Rwz {color: #fff;line-height: 4rem; height: 4rem;padding-right: 0.5rem;font-size: 1.4rem;width: 6rem;}

/*注册*/
.ls-header .ls-header-left a i.icon-close{background:url(../images/guanbi.png) no-repeat center center;display: block;
width: 80%;height: 100%;margin: 0;background-size: 3.0rem auto;}
.regbox{color:#fff;}
.regbox h3{position:relative; width:100%;}
.regbox h3 span{ position:absolute; top:1rem; left:1rem; width:4rem; height:4rem; cursor:pointer;}
.regbox h3 span img{ width:100%; vertical-align:middle;}
.regbox h3 em{width:100%; display:block;}
.regbox h3 em img{ width:100%;}
.regbox ul.reg{margin-bottom: 1rem;}
.regbox ul.reg li{border-bottom:1px solid #323136; font-size:1.4rem; padding:1% 3%; overflow:hidden; width:94%; position:relative;}
.regbox ul.reg li span{ display:inline-block; width:7rem; height:3.0rem; line-height:3.0rem; float:left; width:25%;}
.regbox ul.reg li input{ background:#1a1a22;color:#b2b1b6;height:3.0rem; line-height:3.0rem; float:left;}
.regbox ul.reg li em{ display:inline-block; width:3.0rem;height:3.0rem;cursor:pointer;top:0.5rem;position:absolute; right:1.0rem;}
.regbox ul.reg li em img{ width:100%; vertical-align:middle;}
.regbox ul.reg li em.Get{color:#fee101; width:7rem; line-height:3.0rem; position:absolute;}
.regbox ul.reg li p{float:left;display: inline-block;height: 3.0rem;line-height: 3.0rem;float: left;width: 25%;}
.regbox ul.reg li p i{ font-size:1.2rem;color:#999;}
.selectone{float: left;width: 34%;height: 3rem;line-height: 3rem;margin-right: 1%;background: url(../images/wdown.png) no-repeat right center;overflow: hidden;}
.selectone select{background:none;color:#fff; width:122%;height:3rem; line-height:3rem; font-size:1.5rem;}
.selectone select option{ background:#201f24;}
.Tk{ text-align:center; padding: 2% 0 6% 0; font-size:1.3rem; color:#b2b1b6;}
.Tk a{color:#fee101;}
.downts{ width:100%;}
.downts img{ width:100%;}
.Suc{ margin:3%; background:#262630; color:#fff; height:3.6rem; line-height:3.6rem; border:none; width:94%; font-size:1.5rem;vertical-align:middle;-webkit-border-radius:5px;
-moz-border-radius:5px;border-radius:5px;behavior: url(js/PIE.htc); cursor:pointer;}
.yellow{ background:#fee101;color:#080807;}


/*登录*/
.logbox{padding:1.0rem 2%;}
.logbox ul.loginput{background:#262630;vertical-align:middle;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;behavior: url(js/PIE.htc);}
.logbox ul.loginput li{overflow:hidden; width:96%; padding:1.0rem 2%;font-size:1.4rem;}
.logbox ul.loginput li label{ float:left; width:25%;height:2.6rem; line-height:2.6rem; overflow:hidden;}
.logbox ul.loginput li label i.zicon{background:url(../images/lzh.png) no-repeat center center;display:inline-block;float:left;
width: 2rem;height:100%;margin: 0;background-size: 1.3rem auto;}
.logbox ul.loginput li label i.micon{ background:url(../images/mm.png) no-repeat center center;display:inline-block;float:left;
width: 2rem;height:100%;margin: 0;background-size: 1.3rem auto;}
.logbox ul.loginput li label em{ line-height:100%;}
.logbox ul.loginput li input{ float:left;background:#262630; width:50%; height:2.6rem; line-height:2.6rem;color:#fff;}
.logbox p{overflow:hidden; padding:1.0rem 0;}
.logbox p a{color:#c9c9c9;font-size:1.1rem;}
.logbox p .rega{float:left;}
.logbox p .forgetm{float:right;}

/*交易*/
.Tradebox h3{ background:#1a1a22; width:96%; padding:0.8rem 2%; font-weight:normal;}
.Tradebox h3 span{ width:32%; display:inline-block; text-align:center;color:#999; font-size:1.2rem;}
.Tradebox .Tradeone{ padding:2.0rem 3%; background:#21212b;}
.Tradeone .Uinput{ overflow:hidden;}
.Tradeone .Uinput .Upleft{ float:left; width:50%; margin-right:5%; font-size:1.3rem;}
.Uinput .Upleft p.Uinp{position:relative;width: 120%;}
.Uinput .Upleft p.Uinp span{ position:absolute; left:0; height:2rem; width:2rem; padding:0.3rem 3%;}
.Uinput .Upleft p.Uinp em{ position:absolute; right:0; height:2rem; width:2rem; padding:0.3rem 3%; background: #292935;}
.Uinput .Upleft p.Uinp span img{ width:100%;}
.Uinput .Upleft p.Uinp em img{width:100%;}
.Uinput .Upleft p.Uinp input{background:#292935;color:#096ed8; font-size:1.3rem; height:2.6rem; line-height:2.6rem;width: 100%;}
.Uinput .Upleft .Unum{ overflow:hidden; padding-top:1rem; width:120%;}
.Uinput .Upleft .Unum p{background:#292935; height:2.6rem; line-height:2.6rem; float:left; width:35%;color:#979795; padding:0 2%; position:relative;}
.Uinput .Upleft .Unum p span.bnum{ position:absolute; right:4%;color:#0081ff;}
.Uinput .Upright{ float:right;width:36%;background:#292935; padding:0.8rem 1%;}
.Uinput .Upright p{ overflow:hidden;}
.Uinput .Upright p em{float:right;height:2.0rem; line-height:2.0rem;color:#fff; font-size:1.3rem;}
.Uinput .Upright p span{ float:left;height:2.0rem;width:2.0rem;line-height:2.0rem;color:#fff;font-size:1.3rem;}
.Uinput .Upright p span.green{display:inline-block; margin-right:0.5rem; text-align:center;color:#fff; background:#46d89a; 
font-size:1.2rem;}
.Uinput .Upright p span.red{ background:#f40642;}
.Tradetwo{ background:#21212b; width:94%;}
.Tradetwo p{ height:5rem; line-height:2.5rem;width:31%; display:inline-block; text-align:center;color:#fff; font-size:1.4rem;vertical-align:middle;-webkit-border-radius:5px;
-moz-border-radius:5px;border-radius:5px;behavior: url(js/PIE.htc);}
.Tradetwo p.red{background:#f40642;}
.Tradetwo p.green{background:#46d89a;}
.Tradetwo p.grey{background:#dcdcdc;color:#111;}
.Tradetwo p span{ display:block; margin:0 5%;}
.Tradetwo p span.line{border-bottom:1px solid #fff;}
.Tradetwo p span.bline{border-bottom:1px solid #111;}
.Tratable ul.Tratit{ background:#262630; border:1px solid #1c1c26; overflow:hidden; padding:0 2%; font-size:1.4rem;}
.Tratable ul.Tratit li{float:left; width:25%; text-align:center; display:block;border-bottom:3px solid #262630; padding:1.2rem 0; cursor:pointer;}
.Tratable ul.Tratit li a{color:#fff;}
.Tratable ul.Tratit li.select{border-bottom:3px solid #0080fd;color:#0080fd;}
.Giftrecord{width:100%;background:#21212b;}
.Giftrecord dl{border-bottom:1px solid #1a1a22}
.Giftrecord dl dt{ width:100%; padding:1.5rem 0; cursor:pointer;}
.Giftrecord dl:hover{ background:#292935;}
.Giftrecord dl dt span{width:17%; display:inline-block; text-align:center; font-size:1.3rem;color:#fff;}
.Giftrecord dl dt span.name{ width:22%;}
.Giftrecord dl dt span.red{color:#f40642;}
.Giftrecord dl dt span.green{color:#46d89a;}
.but{ overflow:hidden; padding:1.0rem 2%; } 
.but input[type="button"]{ background:#0081ff; color:#fff; display:inline-block; float:right; margin-left:1.0rem; height:2.4rem; line-height:2.4rem; width:6rem; 
text-align:center;vertical-align:middle;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;behavior: url(js/PIE.htc); cursor:pointer;}
.Giftit dl dt{ width:100%; padding:1.0rem 0;}
.Giftit dl:hover{background:#21212b;}
.Giftit dl dt span{color:#999;}
.Giftit dl dt span.red{color:#999;}

/*我的**/
.search ul{ overflow:hidden;}
.search ul li{font-size:1.4rem; padding:1.5rem 2%; font-weight:normal;border-bottom:1px solid #1a1a22; overflow:hidden; background:#25252f;color: #999999;}
.search ul li a{ display:block;color:#fff;}
.search ul li span.rarrow{ float:right;display: block;width:1.8rem;height:1.8rem;line-height:1.8rem;}
.search ul li span.rarrow i.rimg{background:url(../images/next.png) no-repeat center center;display: block;width: 80%;height: 100%; margin: 0;background-size:0.6em auto;}
.search ul li span.txnum{ float:right; padding-right:1rem; display:inline-block;}
.My ul li{border-bottom:8px solid #1a1a22;}
.My ul li span{ float:left;}
.My ul li span.WHead{ padding-right:1rem;}
.My ul li span.WHead img{display:inline-block; height:5rem; width:5rem;vertical-align:middle;-webkit-border-radius:50%;
-moz-border-radius:50%;border-radius:50%;behavior: url(js/PIE.htc); position:relative;}
.My  ul li p{ float:left; font-size:1.2rem; line-height:200%;}
.My  ul li p em{color:#acacae;}
.search ul li span.Wicon{ display:inline-block; vertical-align:middle; padding-right:1rem; width:6%;}
.search ul li span.Wicon img{ width:100%;}
.czbutton{display:inline-block;text-align:center; background:#0081ff;vertical-align:middle;-webkit-border-radius:5px;color:#fff; width:94%; margin:1.5rem 3%; text-align:center; line-height:3.6rem; 
height:3.6rem;-moz-border-radius:5px;border-radius:5px;behavior: url(js/PIE.htc); cursor:pointer;font-size:1.5rem; cursor:pointer;}
.Bbutton{ background:#262630;}


/*资金*/
.My ul li .gnum{ text-align:center; line-height:200%; font-size:2.0rem;}
.My ul li .gnum i{ font-size:1.4rem; padding-left:0.5rem;}
.My ul li .gnumd{color:#fff; font-weight:bold;}
.My ul li .gnumd i{font-weight:normal;}

/**底部**/
.homebottom{position:fixed; width:100%; height:5rem; bottom:0; background:#201f24;}
.homebottom ul{ overflow:hidden;}
.homebottom ul li{ float:left; width:20%; text-align:center;}
.homebottom ul li a{ display:block; padding-top:0.2rem; color:#6e7275;}
.homebottom ul li a i{ display:block; margin:0 auto; width:2.6rem; height:2.6rem;}
.homebottom ul li.nav01 a i{ background:url(../images/hq.png) no-repeat center center; background-size:100% auto;}
.homebottom ul li.nav02 a i{ background:url(../images/zx.png) no-repeat center center; background-size:100% auto;}
.homebottom ul li.nav03 a i{ background:url(../images/jy.png) no-repeat center center; background-size:100% auto;}
.homebottom ul li.nav04 a i{ background:url(../images/zj.png) no-repeat center center; background-size:100% auto;}
.homebottom ul li.nav05 a i{ background:url(../images/wd.png) no-repeat center center; background-size:100% auto;}
.homebottom ul li.nav01 a.on i{ background:url(../images/hq_select.png) no-repeat center center; background-size:100% auto;}
.homebottom ul li.nav02 a.on i{ background:url(../images/zx_select.png) no-repeat center center; background-size:100% auto;}
.homebottom ul li.nav03 a.on i{ background:url(../images/jy_select.png) no-repeat center center; background-size:100% auto;}
.homebottom ul li.nav04 a.on i{ background:url(../images/zj_select.png) no-repeat center center; background-size:100% auto;}
.homebottom ul li.nav05 a.on i{ background:url(../images/wd_select.png) no-repeat center center; background-size:100% auto;}
.homebottom ul li a span{ display:block; font-size:1.2rem; padding:0.1rem 0;}
.homebottom ul li a.on span{ color:#3492dc;}

